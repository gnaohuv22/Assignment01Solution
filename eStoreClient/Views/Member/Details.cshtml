@model BusinessObject.MemberDto

@{
    ViewData["Title"] = "Member Details";
}

<h1>Member Details</h1>

<div>
    <h4>Member</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Member ID</dt>
        <dd class="col-sm-10">@Model.MemberId</dd>

        <dt class="col-sm-2">Email</dt>
        <dd class="col-sm-10">@Model.Email</dd>

        <dt class="col-sm-2">Company Name</dt>
        <dd class="col-sm-10">@Model.CompanyName</dd>

        <dt class="col-sm-2">City</dt>
        <dd class="col-sm-10">@Model.City</dd>

        <dt class="col-sm-2">Country</dt>
        <dd class="col-sm-10">@Model.Country</dd>
    </dl>
    <p>
        <a asp-action="Edit" asp-route-id="@Model.MemberId" class="btn btn-warning">Edit</a>
        <a asp-action="Index" class="btn btn-primary">Back to List</a>
    </p>
</div>
